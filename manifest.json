{
    "manifest_version": 3,
    "name": "Tab Sleep Timer",
    "version": "1.0",
    "description": "Close tabs after a specified amount of time.",
    "permissions": ["tabs", "alarms"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}